{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "TemporalDistributions": {
    "baseurl": "ftp://hdsc.nws.noaa.gov/",
    "resources": {
      "6h_duration": "pub/hdsc/data/sa/sa_general_6h_temporal.csv",
      "24h_duration": "pub/hdsc/data/sa/sa_general_24h_temporal.csv"
    }
  },
  "APIConfigSettings": {
    "Resources": {
      "Sites": {
        "Description": { "string": "The sites resource represents the hydrologic model used to calculate peak discharge." },
        "uris": {
          "GET": {
            "TR55": {
              "Name": "TR55",
              "Description": { "string": "### TR55 Site Resources\nReturns a dictionary of input and output parameters related to the TR55 equation." }

            },
            "/TR55/GetResult": {
              "Name": "TR55 Hydrograph",
              "Description": { "string": "### TR55 Site Resources\nReturns an unsorted dictionary of time steps (as percent) and associated input and output parameters related to the TR55 equation." }

            },
            "RationalMethod": {
              "Name": "RationalMethod",
              "Description": { "string": "### RationalMethod Site Resources\nReturns a dictionary of input and output parameters related to the Rational Method equation." }
            }
          }
        }
      },
      "States": {
        "Description": { "string": "" },
        "uris": {
          "GET": {
            "/": {
              "Name": "Available States",
              "Description": { "string": "CO" }
            }
          }
        }
      }
    },
    "Parameters": {
      "area": {
        "Description": "Drainage area."
      },
      "precip": {
        "Description": "Precipitation, in inches"
      },
      "precipint": {
        "Description": "Precipitation intensity, in inches per hour"
      },
      "crvnum": {
        "Description": "Runoff curve number"
      },
      "rcoeff": {
        "Description": "Runoff coefficient for Colorado"
      },
      "pdur": {
        "Description": "Storm duration, in hours"
      }
    }
  }
}